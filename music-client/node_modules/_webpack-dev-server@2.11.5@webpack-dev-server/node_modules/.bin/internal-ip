#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_internal-ip@1.2.0@internal-ip/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_internal-ip@1.2.0@internal-ip/cli.js" "$@"
  ret=$?
fi
exit $ret
