#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_uglify-js@3.4.10@uglify-js/bin/uglifyjs" "$@"
  ret=$?
else 
  node  "$basedir/../../../_uglify-js@3.4.10@uglify-js/bin/uglifyjs" "$@"
  ret=$?
fi
exit $ret
