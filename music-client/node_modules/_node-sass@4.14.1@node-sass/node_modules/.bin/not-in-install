#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_in-publish@2.0.1@in-publish/not-in-install.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_in-publish@2.0.1@in-publish/not-in-install.js" "$@"
  ret=$?
fi
exit $ret
