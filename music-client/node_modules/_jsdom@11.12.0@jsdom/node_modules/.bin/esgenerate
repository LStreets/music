#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_escodegen@1.14.3@escodegen/bin/esgenerate.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_escodegen@1.14.3@escodegen/bin/esgenerate.js" "$@"
  ret=$?
fi
exit $ret
